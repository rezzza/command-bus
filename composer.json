{
    "name": "rezzza/command-bus",
    "type": "library",
    "description": "Easy command bus",
    "keywords": ["command", "bus"],
    "license": "MIT",
    "homepage": "https://github.com/rezzza/command-bus",
    "authors": [{
        "name": "Contributor Guys",
        "homepage": "https://github.com/rezzza/command-bus/contributors"
    }],
    "require": {
        "php": ">=5.5.0",
        "psr/log": "~1.0",
        "symfony/event-dispatcher": "~2.5|~3.0",
        "symfony/serializer": "~2.5|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Rezzza\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rezzza\\CommandBus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "require-dev": {
        "atoum/atoum": "^2.5"
    }
}
